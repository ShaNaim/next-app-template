    "husky": "^9.1.7",
    "lint-staged": "^15.3.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
